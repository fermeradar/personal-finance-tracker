{
  "rules": {
    "no-unused-vars": ["warn", { 
      "argsIgnorePattern": "^_", 
      "varsIgnorePattern": "^_" 
    }],
    "no-undef": "warn",
    "no-console": "warn",
    "no-case-declarations": "warn",
    "no-process-exit": "warn",
    "no-const-assign": "warn",
    "no-shadow-restricted-names": "warn",
    "no-dupe-keys": "warn"
  }
}